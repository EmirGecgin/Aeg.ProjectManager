                    <div class="card-body">
                        <div class="card-title d-flex align-items-start justify-content-between">
                            <div class="avatar flex-shrink-0">
                                <img src="~/Theme/assets/img/icons/unicons/chart-success.png" alt="chart success" class="rounded">
                            </div>
                        </div>
                        <span class="fw-semibold d-block mb-1">Tamamlanan Projeler</span>
                        <small class="text-success fw-semibold"><i class="bx bx-up-arrow-alt"></i> @ViewBag.DoneProjectCount Proje</small>
